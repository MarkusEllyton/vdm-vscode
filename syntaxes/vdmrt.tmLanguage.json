{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "VDM-RT",
	"patterns": [
		{
			"include": "source.vdm"
		},
		{
			"include": "source.vdmpp"
		},
		{
			"name": "storage.type.system.vdmrt",
			"match": "\\b(system)\\b"
		},
		{
			"name": "keyword.other.vdmrt",
			"match": "\\b(async|time)\\b"
		},
		{
			"name": "support.function.timing.vdmrt",
			"match": "\\b(cycles|duration)\\b"
		}
	],
	"scopeName": "source.vdmrt"
}
