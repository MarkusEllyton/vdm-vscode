{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "VDM++",
	"patterns": [
		{
			"include": "source.vdm"
		},
		{
			"name": "storage.type.class.vdmpp",
			"match": "\\b(class)\\b"
		},
		{
			"name": "storage.type.definition.class.vdmpp",
			"match": "\\b(thread|sync)\\b"
		}
	],
	"scopeName": "source.vdmpp"
}
